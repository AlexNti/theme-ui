{
  ".token.comment,\n.token.prolog,\n.token.doctype,\n.token.cdata": {
    "color": "#aaa"
  },
  ".token.punctuation": {
    "color": "#999"
  },
  ".namespace": {
    "opacity": ".7"
  },
  ".token.property,\n.token.tag,\n.token.boolean,\n.token.number,\n.token.constant,\n.token.symbol": {
    "color": "#0cf"
  },
  ".token.selector,\n.token.attr-name,\n.token.string,\n.token.char,\n.token.builtin": {
    "color": "yellow"
  },
  ".token.operator,\n.token.entity,\n.token.url,\n.language-css .token.string,\n.token.variable,\n.token.inserted": {
    "color": "yellowgreen"
  },
  ".token.atrule,\n.token.attr-value,\n.token.keyword": {
    "color": "deeppink"
  },
  ".token.regex,\n.token.important": {
    "color": "orange"
  },
  ".token.important,\n.token.bold": {
    "fontWeight": "bold"
  },
  ".token.italic": {
    "fontStyle": "italic"
  },
  ".token.entity": {
    "cursor": "help"
  },
  ".token.deleted": {
    "color": "red"
  }
}
