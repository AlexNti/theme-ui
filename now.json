{
  "version": 2,
  "name": "theme-ui",
  "alias": "theme-ui.now.sh",
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "docs/public"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/docs/$1" }
  ]
}
